{
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "aboutdevwaghela.wealthwire.me"
        }
      ],
      "destination": "https://devwaghela.vercel.app/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/demo",
      "destination": "/demo.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
